    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.6.2",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.15",
    "concurrently": "^9.2.1",
    "electron-builder": "^24.6.3",
    "postcss": "^8.4.28",
